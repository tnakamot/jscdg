{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/tnakamot/jscdg/src/test/resources/example1_schema.json",
  "title": "Example Data Structure",
  "description": "Example data structure to test JSON Schema Code and Document Generator.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "title": "title",
      "description": "Name of this datd",
      "default": "test",
      "examples": "Help!"
    },
    "num": {
      "type": "integer",
      "description": "test integer",
      "minimum": 5,
      "examples": [5]
    },
    "num2": {
      "type": "number",
      "description": "test number",
      "minimum": 5,
      "examples": [5.2, 1e10]
    },
    "yes_or_no": {
      "type": "boolean",
      "description": "test boolean",
      "default": true,
      "examples": [true, false]
    },
    "testObj": {
      "type": "object",
      "description": "aaaa",
      "properties": {
        "name": {
          "type": "string",
          "description": "OK"
        }
      }
    }
  },
  "required": [ "name" ]
}